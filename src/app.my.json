{
  "pages": [
    "pages/index/index"
  ],
  "subPackages": [
    {
      "root": "sourceSubPackage",
      "pages": [
        "index"
      ]
    }
  ],
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/index/index"
      }
    ]
  },
  "window": {
    "navigationBarTextStyle": "black",
