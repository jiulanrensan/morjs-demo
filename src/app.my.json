  },
  "usingComponents": {
    "pagoda-custom-tab-bar": "/components/customTabBar/index"
  },
  "resolveAlias": {
    "~/*": "/*"
  },
  "sitemapLocation": "sitemap.json",
  "globalObjectMode": "enable",
  "requirePolyfill": true
}